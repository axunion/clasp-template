{
  "name": "post-to-spreadsheet",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"**/*.+(html|css|js|json|yml|ts)\"",
    "format:check": "prettier --check \"**/*.+(html|css|js|json|yml|ts)\"",
    "lint": "eslint --ext .js,.ts src/",
    "validate": "npm run format:check && npm run lint",
    "test": "vitest"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/google-apps-script": "^1.0.83",
    "eslint": "^9.5.0",
    "globals": "^15.5.0",
    "prettier": "3.3.1",
    "typescript-eslint": "^7.13.0",
    "vitest": "^1.6.0"
  }
}
